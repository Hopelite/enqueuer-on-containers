{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TelegramClient": {
    "AccessToken": "{ACCESS_TOKEN}"
  },
  "ConnectionStrings": {
    "EventBus": "{CONNECTION_STRING}"
  },
  "EventBus": {
    "SubscriptionClientName": "Telegram_Notifications"
  },
  "NotificationsDatabase": {
    "ConnectionString": "{CONNECTION_STRING}",
    "DatabaseName": "NotificationsDB"
  }
}
