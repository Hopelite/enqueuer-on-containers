{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "IdentityDB": "{CONNECTION_STRING}"
  },
  "OAuth": {
    "Issuer": "https://localhost:7279",
    "TokenLifetimeInSeconds": 3600
  },
  "KeyVault": {
    "Url": "{KEY_VAULT_URL}"
  }
}
